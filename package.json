    "path-to-regexp": "6.2.0",
    "polished": "^4.1.3",
    "query-string": "^7.0.1",
    "react": "17.0.2",
    "react-document-title": "^2.0.3",
    "react-dom": "17.0.2",
    "react-intl-universal": "2.4.8",
    "react-json-view": "1.21.3",
    "react-router-dom": "5.2.0",
    "react-virtualized": "9.22.3",
    "recoil": "^0.4.1",
    "socket.io-client": "2.3.0",
    "socketio-wildcard": "2.0.0",
