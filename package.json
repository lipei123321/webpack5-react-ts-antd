    "fork-ts-checker-webpack-plugin": "6.2.13",
    "fs-extra": "10.0.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "27.0.6",
