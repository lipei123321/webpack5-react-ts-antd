    "less-loader": "10.0.1",
    "lint-staged": "11.1.1",
    "mini-css-extract-plugin": "1.3.1",
    "moment": "^2.29.1",
    "moment-locales-webpack-plugin": "1.2.0",
    "postcss": "8.3.6",
    "postcss-loader": "6.1.1",
    "prettier": "2.3.2",
    "prettier-stylelint": "^0.4.2",
